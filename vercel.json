{
  "version": 2,
  "builds": [
    {
      "src": "devratedraft/**",
      "use": "@vercel/static" 
    },
    {
      "src": "devratebe/service.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
   
    {
      "src": "/api/(.*)",
      "dest": "devratebe/service.js" 
    },

    {
      "src": "/(.*)",
      "dest": "devratedraft/$1" 
    }
  ]
}